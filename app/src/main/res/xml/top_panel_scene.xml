<?xml version="1.0" encoding="utf-8"?>
<MotionScene
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto">

  <Transition
    android:id="@+id/transition"
    app:constraintSetEnd="@+id/end"
    app:constraintSetStart="@+id/start"
    app:interpolator="linear"
    app:duration="1000">

    <OnSwipe
      app:dragDirection="dragDown"
      app:touchAnchorId="@+id/top_container"
      app:touchAnchorSide="top" />

    <KeyFrameSet>
      <KeyPosition
        app:framePosition="33"
        app:keyPositionType="parentRelative"
        app:percentX="0.6"
        app:target="@+id/imgMiddleLeft" />

      <KeyPosition
        app:framePosition="66"
        app:keyPositionType="parentRelative"
        app:percentX="0.8"
        app:target="@+id/imgMiddleMiddle" />

    </KeyFrameSet>
  </Transition>

  <ConstraintSet android:id="@+id/start">
    <Constraint
      android:id="@+id/imgTopLeft"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="32dp"
      app:icon="@drawable/ic_airplane"
      app:layout_constraintEnd_toStartOf="@+id/imgTopMiddle"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

    <Constraint
      android:id="@+id/imgTopMiddle"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginTop="32dp"
      app:icon="@drawable/ic_wifi"
      app:layout_constraintEnd_toStartOf="@+id/imgTopRight"
      app:layout_constraintStart_toEndOf="@+id/imgTopLeft"
      app:layout_constraintTop_toTopOf="parent" />

    <Constraint
      android:id="@+id/imgTopRight"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginTop="32dp"
      app:icon="@drawable/ic_android"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imgTopMiddle"
      app:layout_constraintTop_toTopOf="parent" />

    <Constraint
      android:id="@+id/imgMiddleLeft"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="64dp"
      app:icon="@drawable/ic_bluetooth"
      app:layout_constraintEnd_toStartOf="@+id/imgMiddleMiddle"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imgTopLeft" />

    <Constraint
      android:id="@+id/imgMiddleMiddle"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginTop="64dp"
      app:icon="@drawable/ic_brightness"
      app:layout_constraintEnd_toStartOf="@+id/imgMiddleRight"
      app:layout_constraintStart_toEndOf="@+id/imgMiddleLeft"
      app:layout_constraintTop_toBottomOf="@+id/imgTopMiddle" />

    <Constraint
      android:id="@+id/imgMiddleRight"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginTop="64dp"
      app:icon="@drawable/ic_flash"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imgMiddleMiddle"
      app:layout_constraintTop_toBottomOf="@+id/imgTopRight" />

  </ConstraintSet>

  <ConstraintSet android:id="@+id/end">
    <Constraint
      android:id="@+id/imgTopLeft"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      app:icon="@drawable/ic_airplane"
      app:layout_constraintEnd_toStartOf="@+id/imgTopMiddle"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
    <Constraint
      android:id="@+id/imgTopMiddle"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      app:icon="@drawable/ic_wifi"
      app:layout_constraintEnd_toStartOf="@+id/imgTopRight"
      app:layout_constraintStart_toEndOf="@+id/imgTopLeft"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
    <Constraint
      android:id="@+id/imgTopRight"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      app:icon="@drawable/ic_android"
      app:layout_constraintEnd_toStartOf="@+id/imgMiddleLeft"
      app:layout_constraintStart_toEndOf="@+id/imgTopMiddle"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
    <Constraint
      android:id="@+id/imgMiddleLeft"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      app:icon="@drawable/ic_bluetooth"
      app:layout_constraintEnd_toStartOf="@+id/imgMiddleMiddle"
      app:layout_constraintStart_toEndOf="@+id/imgTopRight"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
    <Constraint
      android:id="@+id/imgMiddleMiddle"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="8dp"
      app:icon="@drawable/ic_brightness"
      app:layout_constraintEnd_toStartOf="@+id/imgMiddleRight"
      app:layout_constraintStart_toEndOf="@+id/imgMiddleLeft"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
    <Constraint
      android:id="@+id/imgMiddleRight"
      android:layout_width="40dp"
      android:layout_height="40dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="8dp"
      android:layout_marginBottom="8dp"
      android:layout_marginEnd="16dp"
      app:icon="@drawable/ic_flash"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/imgMiddleMiddle"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent" />
  </ConstraintSet>

</MotionScene>